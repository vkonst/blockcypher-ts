{
  "name": "blockcypher-ts",
  "version": "1.0.0",
  "description": "A REST API and Websocket client for the BlockCypher Web services",
  "main": "dist/blockcypher.js",
  "types": "dist/",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist && rimraf coverage",
    "prebuild": "npm run clean",
    "generateDocs": "jsdoc -d JSDoc ./dist/src/blockcypher.js",
    "test": "mocha --require ts-node/register ./test/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vkonst/blockcypher-ts.git"
  },
  "keywords": [
    "blockchain",
    "cryptocurrenceies",
    "blockcypher",
    "websocket"
  ],
  "author": "vadim.konstantinov@gmail.com",
  "contributors": [ "Dmitry Tverdov (https://github.com/DTverdov)" ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vkonst/blockcypher-ts/issues"
  },
  "homepage": "https://github.com/vkonst/blockcypher-ts#readme",
  "dependencies": {
    "debug": "^3.1.0",
    "events": "^2.0.0",
    "request": "^2.83.0",
    "websocket": "^1.0.25"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/debug": "0.0.30",
    "@types/events": "^1.1.0",
    "@types/expect": "^1.20.2",
    "@types/mocha": "^2.2.48",
    "@types/pluralize": "0.0.28",
    "@types/request": "^2.47.0",
    "@types/should": "^13.0.0",
    "@types/sinon": "^4.1.3",
    "@types/typescript": "^2.0.0",
    "@types/websocket": "0.0.36",
    "chai": "^4.1.2",
    "expect": "^22.2.0",
    "mocha": "^5.0.0",
    "pluralize": "^7.0.0",
    "rimraf": "^2.0.0",
    "should": "^13.2.1",
    "sinon": "^4.2.2",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  }
}
